<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/fee_color"
    android:orientation="vertical">


   <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:orientation="vertical"
       >
       <include layout="@layout/title_layout"/>

    <RelativeLayout
        android:id="@+id/relView1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp_10"
                android:gravity="center"
                android:text="修改密码"
                android:textColor="#151413"
                android:textSize="@dimen/sp_16"
                android:textStyle="normal" />


        </LinearLayout>

        <ImageView
            android:layout_width="@dimen/dp_15"
            android:layout_height="@dimen/dp_15"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/jiantouyou" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relView2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp_10"
                android:gravity="center"
                android:text="常见问题"
                android:textColor="#151413"
                android:textSize="@dimen/sp_16"
                android:textStyle="normal" />


        </LinearLayout>

        <ImageView
            android:layout_width="@dimen/dp_15"
            android:layout_height="@dimen/dp_15"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/jiantouyou" />


    </RelativeLayout>




    <RelativeLayout
        android:id="@+id/relView3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp_10"
                android:gravity="center"
                android:text="意见反馈"
                android:textColor="#151413"
                android:textSize="@dimen/sp_16"
                android:textStyle="normal" />


        </LinearLayout>

        <ImageView
            android:layout_width="@dimen/dp_15"
            android:layout_height="@dimen/dp_15"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/jiantouyou" />


    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/relView4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp_10"
                android:gravity="center"
                android:text="清除缓存"
                android:textColor="#151413"
                android:textSize="@dimen/sp_16"
                android:textStyle="normal" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_5"
                android:gravity="center"
                android:text="0.00KB"
                android:textColor="#1678FF"
                android:textSize="@dimen/sp_16"
                android:textStyle="normal" />

            <ImageView
                android:layout_width="@dimen/dp_15"
                android:layout_height="@dimen/dp_15"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:src="@drawable/jiantouyou" />


        </LinearLayout>


    </RelativeLayout>



    <RelativeLayout
        android:id="@+id/relView5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp_10"
                android:gravity="center"
                android:text="关于我们"
                android:textColor="#151413"
                android:textSize="@dimen/sp_16"
                android:textStyle="normal" />


        </LinearLayout>

        <ImageView
            android:layout_width="@dimen/dp_15"
            android:layout_height="@dimen/dp_15"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/jiantouyou" />


    </RelativeLayout>



    <RelativeLayout
        android:id="@+id/relViewCui1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp_10"
                android:gravity="center"
                android:text="用户协议"
                android:textColor="#151413"
                android:textSize="@dimen/sp_16"
                android:textStyle="normal" />


        </LinearLayout>

        <ImageView
            android:layout_width="@dimen/dp_15"
            android:layout_height="@dimen/dp_15"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/jiantouyou" />


    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/relViewCui2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp_10"
                android:gravity="center"
                android:text="隐私政策"
                android:textColor="#151413"
                android:textSize="@dimen/sp_16"
                android:textStyle="normal" />


        </LinearLayout>

        <ImageView
            android:layout_width="@dimen/dp_15"
            android:layout_height="@dimen/dp_15"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/jiantouyou" />


    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/relViewCui3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45"
        android:layout_marginLeft="@dimen/dp_10"
        android:visibility="gone"
        android:layout_marginRight="@dimen/dp_10">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dp_10"
                android:gravity="center"
                android:text="注销用户"
                android:textColor="#333"
                android:textSize="@dimen/sp_14"
                android:textStyle="normal" />


        </LinearLayout>

        <ImageView
            android:layout_width="@dimen/dp_15"
            android:layout_height="@dimen/dp_15"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/jiantouyou" />


    </RelativeLayout>

   </LinearLayout>

    <TextView
        android:id="@+id/okID"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="116dp"
        android:gravity="center"
        android:text="退出登录"
        android:textColor="#939393"
        android:textSize="@dimen/sp_16" />
</RelativeLayout>
