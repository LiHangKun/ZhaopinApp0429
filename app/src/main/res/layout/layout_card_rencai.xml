<?xml version="1.0" encoding="utf-8"?>
<com.lx.zhaopin.view.dragcard.AntiAliasView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center_horizontal"
    android:layout_marginLeft="18dp"
    android:layout_marginTop="10dp"
    android:layout_marginRight="@dimen/dp_18"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="@drawable/shap_rect_white_5dwh"
        android:padding="5dp">


        <LinearLayout
            android:id="@+id/llViewALL"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/llViewGongSi"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_80"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_10"
                android:orientation="horizontal">

                <com.makeramen.roundedimageview.RoundedImageView
                    android:id="@+id/roundedImageView"
                    android:layout_width="@dimen/dp_60"
                    android:layout_height="@dimen/dp_60"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:layout_gravity="center_vertical"
                    android:scaleType="centerCrop"
                    android:src="@drawable/splashbj"
                    app:riv_border_width="0dip"
                    app:riv_corner_radius_bottom_left="@dimen/dp_5"
                    app:riv_corner_radius_bottom_right="@dimen/dp_5"
                    app:riv_corner_radius_top_left="@dimen/dp_5"
                    app:riv_corner_radius_top_right="@dimen/dp_5"
                    app:riv_mutate_background="true"
                    app:riv_oval="false" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dp_5"
                        android:layout_marginRight="@dimen/dp_8"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_40"
                            android:orientation="horizontal">


                            <TextView
                                android:id="@+id/tv1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:singleLine="true"
                                android:text="111111"
                                android:textColor="#333"
                                android:textSize="@dimen/sp_14" />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_40"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv2"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:ellipsize="end"
                                android:gravity="center_vertical"
                                android:maxLength="8"
                                android:singleLine="true"
                                android:text="1111"
                                android:textColor="#3D3D3D"
                                android:textSize="@dimen/sp_12" />

                            <View
                                android:layout_width="1px"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/dp_5"
                                android:layout_marginTop="@dimen/dp_15"
                                android:layout_marginRight="@dimen/dp_5"
                                android:layout_marginBottom="@dimen/dp_15"
                                android:background="#e7e7e7" />

                            <TextView
                                android:id="@+id/tv3"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:ellipsize="end"
                                android:gravity="center_vertical"
                                android:maxLength="8"
                                android:singleLine="true"
                                android:text="1111"
                                android:textColor="#3D3D3D"
                                android:textSize="@dimen/sp_12" />

                            <View
                                android:layout_width="1px"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/dp_5"
                                android:layout_marginTop="@dimen/dp_15"
                                android:layout_marginRight="@dimen/dp_5"
                                android:layout_marginBottom="@dimen/dp_15"
                                android:background="#e7e7e7" />


                            <TextView
                                android:id="@+id/tv4"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center_vertical"
                                android:singleLine="true"
                                android:text="1111"
                                android:textColor="#3D3D3D"
                                android:textSize="@dimen/sp_12" />


                            <View
                                android:layout_width="1px"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="@dimen/dp_5"
                                android:layout_marginTop="@dimen/dp_15"
                                android:layout_marginRight="@dimen/dp_5"
                                android:layout_marginBottom="@dimen/dp_15"
                                android:background="#e7e7e7" />


                            <TextView
                                android:id="@+id/tv5"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center_vertical"
                                android:singleLine="true"
                                android:text="金水区"
                                android:textColor="#3D3D3D"
                                android:textSize="@dimen/sp_12" />


                        </LinearLayout>


                    </LinearLayout>


                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/jiantouyou"
                        android:visibility="gone" />


                </LinearLayout>


            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:gravity="center_vertical"
                android:text="工作经验"
                android:textColor="#333"
                android:textSize="@dimen/sp_16"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv6"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/dp_40"
                    android:layout_marginRight="@dimen/dp_5"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="工作经验"
                    android:textColor="#333"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:id="@+id/tv7"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/dp_40"
                    android:layout_marginLeft="@dimen/dp_5"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="工作经验"
                    android:textColor="#333"
                    android:textSize="@dimen/sp_14" />

            </LinearLayout>


            <TextView
                android:id="@+id/tv8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_5"
                android:ellipsize="end"
                android:maxLines="4"
                android:text="是上海络科技有限是1111上海络科技有限公司旗下素材在线交易网站于2016年7月上线运营是上海络科技有限公司旗下素材在线交易网站于2016年7月上线运营公司旗下素材在线交易网站于2016年7月上线运营。主要服务是提供图片、视频、音频、psd源文件等主要服务是提供图片等旗下素材在线交易网站于2016年7月上线运营。主要服务是提供图片、视频、音频、psd源文件等。" />


            <com.lx.zhaopin.view.FlowLiner
                android:id="@+id/flowLiner1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginRight="10dp" />

            <!--<View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginTop="@dimen/dp_5"
                android:background="#e7e7e7" />-->


            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_5"
                android:src="@drawable/shouye_hengxian" />


            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:gravity="center_vertical"
                android:text="毕业院校"
                android:textColor="#333"
                android:textSize="@dimen/sp_16"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv9"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_40"
                    android:layout_marginRight="@dimen/dp_5"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="工作经验"
                    android:textColor="#333"
                    android:textSize="@dimen/sp_14" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_marginTop="@dimen/dp_5"
                    android:background="#e7e7e7" />


            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:gravity="center_vertical"
                android:text="毕业院校"
                android:textColor="#333"
                android:textSize="@dimen/sp_16"
                android:textStyle="bold" />


            <com.lx.zhaopin.view.FlowLiner
                android:id="@+id/flowLiner2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginRight="10dp" />


        </LinearLayout>


    </RelativeLayout>


</com.lx.zhaopin.view.dragcard.AntiAliasView>

